# .env.example

# Database
DATABASE_URL=postgresql://localhost:5432/bid_scoring

# OpenAI API (for embeddings)
OPENAI_API_KEY=sk-...
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_TIMEOUT=30
OPENAI_MAX_RETRIES=2
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIM=1536
BID_SCORING_AGENT_MCP_MODEL=gpt-5-mini
BID_SCORING_AGENT_MCP_TOP_K=8
BID_SCORING_AGENT_MCP_MODE=hybrid
BID_SCORING_AGENT_MCP_MAX_CHARS=320
BID_SCORING_AGENT_MCP_EXECUTION_MODE=tool-calling
BID_SCORING_AGENT_MCP_MAX_TURNS=8
BID_SCORING_AGENT_MCP_POLICY_PATH=config/agent_scoring_policy.yaml

# MinerU API (PDF parsing)
MINERU_PDF_PARSER=auto
MINERU_PDF_COMMAND=
MINERU_OUTPUT_ROOT=.mineru-output
MINERU_PDF_TIMEOUT_SECONDS=1800

MINERU_API_URL=https://mineru.net/api/v4
MINERU_API_KEY=your_mineru_api_key_here
MINERU_API_REQUEST_TIMEOUT_SECONDS=30
MINERU_API_POLL_TIMEOUT_SECONDS=900
MINERU_API_POLL_INTERVAL_SECONDS=5
MINERU_API_UPLOAD_MAX_RETRIES=3

# MinIO Object Storage
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=bids
MINIO_SECURE=false

# Embedding Batch Processing
EMBEDDING_BATCH_SIZE=100
EMBEDDING_MAX_RETRIES=6

# MCP Server Cache Configuration (Optional)
BID_SCORING_RETRIEVER_CACHE_SIZE=32
BID_SCORING_QUERY_CACHE_SIZE=1024
